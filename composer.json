{
    "name": "romanzipp/laravel-validator-pizza",
    "description": "A Laravel Wrapper for the laravel.pizza disposable email API",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "romanzipp",
            "email": "ich@ich.wtf",
            "homepage": "https://ich.wtf"
        }
    ],
    "require": {
        "php": "^7.2|^8.0",
        "guzzlehttp/guzzle": "^6.3|^7.0",
        "illuminate/support": "^5.5|^6.0|^7.0|^8.0|^9.0|^10.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "mockery/mockery": "^1.3.2",
        "orchestra/testbench": ">=3.8",
        "phpstan/phpstan": "^0.12.99|^1.0",
        "phpunit/phpunit": "^8.0|^9.0",
        "romanzipp/php-cs-fixer-config": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "romanzipp\\ValidatorPizza\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "romanzipp\\ValidatorPizza\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "romanzipp\\ValidatorPizza\\Providers\\ValidatorPizzaProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
